   ],
   "source": [
    "# ====== CONFIG ======\n",
    "SYMBOLS_RAW = [\n",
    "    \"ETH\",\"BNB\",\"SOL\",\"XRP\",\"DOGE\",\"ADA\",\"AVAX\",\"TRX\",\"TON\",\"LINK\",\n",
    "    \"MATIC\",\"DOT\",\"HYPE\",\"BCH\",\"XLM\",\"LEO\",\"SUI\",\"XMR\",\"SHIB\",\"UNI\"\n",
    "]\n",
    "START_DATE = \"2025-04-01\"\n",
    "END_DATE   = \"2025-04-10\"\n",
    "\n",
    "OUT_DIR_1S  = \"data_1s\"\n",
    "OUT_DIR_15S = \"data_15s\"\n",
    "\n",
    "# ====== IMPORTS ======\n",
    "import os, time, io, zipfile, gzip, requests\n",
    "from datetime import datetime, timezone, timedelta\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "os.makedirs(OUT_DIR_1S, exist_ok=True)\n",
    "os.makedirs(OUT_DIR_15S, exist_ok=True)\n",
    "\n",
    "BASE_API    = \"https://api.binance.com\"\n",
    "\n",
    "def to_ms(dt_str):\n",
    "    return int(datetime.strptime(dt_str, \"%Y-%m-%d %H:%M:%S\").replace(tzinfo=timezone.utc).timestamp()*1000)\n",
    "\n",
    "START_MS = to_ms(START_DATE + \" 00:00:00\")\n",
    "END_MS   = to_ms(END_DATE   + \" 23:59:59\")\n",
    "\n",
    "# ---------- helpers ----------\n",
    "def list_available_symbols():\n",
    "    \"\"\"Fetch exchangeInfo and return a set of listed symbols (e.g., 'BTCUSDT').\"\"\"\n",
    "    url = f\"{BASE_API}/api/v3/exchangeInfo\"\n",
    "    r = requests.get(url, timeout=20); r.raise_for_status()\n",
    "    data = r.json()\n",
    "    return {s[\"symbol\"] for s in data.get(\"symbols\", [])}\n",
    "\n",
    "def fetch_aggtrades_rest(symbol, start_ms, end_ms, step_ms=3*60*60*1000, pause=0.15):\n",
    "    \"\"\"\n",
    "    REST: /api/v3/aggTrades — кусками по 3 часа.\n",
    "    Возвращает DataFrame с колонками: time_ms, price, qty\n",
    "    \"\"\"\n",
    "    url = f\"{BASE_API}/api/v3/aggTrades\"\n",
    "    out = []\n",
    "    t0 = start_ms\n",
    "    while t0 <= end_ms:\n",
    "        t1 = min(t0 + step_ms - 1, end_ms)\n",
    "        params = {\"symbol\": symbol, \"startTime\": t0, \"endTime\": t1, \"limit\": 1000}\n",
    "        resp = requests.get(url, params=params, timeout=30)\n",
    "        if resp.status_code == 429:\n",
    "            time.sleep(1.0); continue\n",
    "        resp.raise_for_status()\n",
    "        data = resp.json()\n",
    "        if data:\n",
    "            out.extend([(d[\"T\"], float(d[\"p\"]), float(d[\"q\"])) for d in data])\n",
    "        t0 = t1 + 1\n",
    "        time.sleep(pause)\n",
    "    if not out:\n",
    "        return pd.DataFrame(columns=[\"time_ms\",\"price\",\"qty\"])\n",
    "    df = pd.DataFrame(out, columns=[\"time_ms\",\"price\",\"qty\"])\n",
    "    # sanity filter: 2010..2035 years\n",
    "    lo = pd.Timestamp(\"2010-01-01\", tz=\"UTC\").value // 10**6\n",
    "    hi = pd.Timestamp(\"2035-12-31 23:59:59\", tz=\"UTC\").value // 10**6\n",
    "    df = df[(df[\"time_ms\"]>=lo) & (df[\"time_ms\"]<=hi)]\n",
    "    return df\n",
    "\n",
    "def trades_to_1s(df_tr):\n",
    "    \"\"\"Build 1s OHLCV from aggTrades. Index = UTC seconds.\"\"\"\n",
    "    if df_tr.empty: \n",
    "        return pd.DataFrame(columns=[\"open\",\"high\",\"low\",\"close\",\"volume\"])\n",
    "    ts = pd.to_datetime(df_tr[\"time_ms\"], unit=\"ms\", utc=True)\n",
    "    ts_floor = ts.dt.floor(\"S\")\n",
    "    g = df_tr.assign(ts=ts_floor).sort_values([\"ts\",\"time_ms\"]).groupby(\"ts\", sort=True)\n",
    "    out = pd.DataFrame({\n",
    "        \"open\":   g[\"price\"].first(),\n",
    "        \"high\":   g[\"price\"].max(),\n",
    "        \"low\":    g[\"price\"].min(),\n",
    "        \"close\":  g[\"price\"].last(),\n",
    "        \"volume\": g[\"qty\"].sum(),\n",
    "    })\n",
    "    return out\n",
    "\n",
    "def resample_15s(df_1s):\n",
    "    if df_1s.empty: return df_1s\n",
    "    df_1s = df_1s.sort_index()\n",
    "    agg = {\"open\":\"first\",\"high\":\"max\",\"low\":\"min\",\"close\":\"last\",\"volume\":\"sum\"}\n",
    "    df_15s = df_1s.resample(\"15S\", label=\"right\", closed=\"right\").agg(agg)\n",
    "    return df_15s.dropna(subset=[\"open\",\"high\",\"low\",\"close\"])\n",
    "\n",
    "def process_symbol(sym_base, listed_symbols):\n",
    "    symbol = sym_base.upper().rstrip(\"USDT\") + \"USDT\"\n",
    "    if symbol not in listed_symbols:\n",
    "        print(f\"[skip] {symbol}: not listed on Binance (skipping).\")\n",
    "        return\n",
    "    print(f\"[+] {symbol}: fetching aggTrades REST ...\")\n",
    "    df_tr = fetch_aggtrades_rest(symbol, START_MS, END_MS)\n",
    "    if df_tr.empty:\n",
    "        print(f\"[!] {symbol}: no trades in {START_DATE}..{END_DATE}\")\n",
    "        return\n",
    "    df_1s = trades_to_1s(df_tr)\n",
    "    # strict clip\n",
    "    t0 = pd.Timestamp(START_DATE + \" 00:00:00\", tz=\"UTC\")\n",
    "    t1 = pd.Timestamp(END_DATE   + \" 23:59:59\", tz=\"UTC\")\n",
    "    df_1s = df_1s.loc[(df_1s.index>=t0) & (df_1s.index<=t1)]\n",
    "    p1 = os.path.join(OUT_DIR_1S,  f\"{symbol}-1s-{START_DATE}_{END_DATE}.csv\")\n",
    "    p2 = os.path.join(OUT_DIR_15S, f\"{symbol}-15s-{START_DATE}_{END_DATE}.csv\")\n",
    "    df_1s.to_csv(p1,  index_label=\"open_time\")\n",
    "    df_15 = resample_15s(df_1s)\n",
    "    df_15.to_csv(p2, index_label=\"open_time\")\n",
    "    print(f\"[✓] {symbol}: 1s={len(df_1s):,} → 15s={len(df_15):,}\")\n",
    "\n",
    "# ---- run ----\n",
    "listed = list_available_symbols()\n",
    "for s in SYMBOLS_RAW:\n",
    "    process_symbol(s, listed)\n"
   ]
  },
