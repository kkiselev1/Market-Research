      "SOLUSDT     0.939261\n",
      "LINKUSDT    0.937869\n",
      "Name: BTCUSDT, dtype: float64\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x800 with 2 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# === Конфигурация путей и окна ===\n",
    "BTC_DAILY_DIR = \"15s\"        # папка с по-дневным BTC\n",
    "BTC_PATTERN   = \"BTCUSDT-15s-2025-04-0[1-9].csv\"  # 01..09\n",
    "BTC_PATTERN2  = \"BTCUSDT-15s-2025-04-10.csv\"      # 10 число отдельно\n",
    "OTHER_15S_DIR = \"data_15s\"                        # 15s для альткоинов\n",
    "START_DATE    = \"2025-04-01\"\n",
    "END_DATE      = \"2025-04-10\"\n",
    "BENCH         = \"BTCUSDT\"\n",
    "\n",
    "import os, glob\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# --- утилиты ---\n",
    "def ensure_utc_index(df, col=\"open_time\"):\n",
    "    \"\"\"Приводим колонку времени к datetime64[ns, UTC] и делаем индексом.\"\"\"\n",
    "    if col in df.columns:\n",
    "        dt = pd.to_datetime(df[col], utc=True, errors=\"coerce\")\n",
    "        df = df.dropna(subset=[col]).copy()\n",
    "        df.index = dt\n",
    "        df = df.drop(columns=[col])\n",
    "    elif isinstance(df.index, pd.DatetimeIndex):\n",
    "        if df.index.tz is None:\n",
    "            df.index = df.index.tz_localize(\"UTC\")\n",
    "        else:\n",
    "            df.index = df.index.tz_convert(\"UTC\")\n",
    "    else:\n",
    "        raise ValueError(\"Нет столбца open_time и индекс не datetime.\")\n",
    "    return df.sort_index()\n",
    "\n",
    "def load_btc_daily_concat():\n",
    "    \"\"\"Склеивает BTCUSDT по дневным файлам 01..10 апреля (15s CSV).\"\"\"\n",
    "    paths = sorted(glob.glob(os.path.join(BTC_DAILY_DIR, BTC_PATTERN))) \\\n",
    "          + sorted(glob.glob(os.path.join(BTC_DAILY_DIR, BTC_PATTERN2)))\n",
    "    if not paths:\n",
    "        raise SystemExit(f\"[!] Не найдено BTC-файлов по шаблонам в {BTC_DAILY_DIR}\")\n",
    "    frames = []\n",
    "    for p in paths:\n",
    "        df = pd.read_csv(p, usecols=[\"open_time\",\"close\"])\n",
    "        df = ensure_utc_index(df, \"open_time\")\n",
    "        frames.append(df.rename(columns={\"close\": BENCH})[[BENCH]])\n",
    "    btc = pd.concat(frames).sort_index()\n",
    "    # строгий клип по окну\n",
    "    t0 = pd.Timestamp(START_DATE + \" 00:00:00\", tz=\"UTC\")\n",
    "    t1 = pd.Timestamp(END_DATE   + \" 23:59:59\", tz=\"UTC\")\n",
    "    return btc.loc[(btc.index>=t0) & (btc.index<=t1)]\n",
    "\n",
    "def load_other_symbol(symbol):\n",
    "    \"\"\"Читает один альт из data_15s (единый файл за 01..10). Возвращает колонку close.\"\"\"\n",
    "    path = os.path.join(OTHER_15S_DIR, f\"{symbol}-15s-{START_DATE}_{END_DATE}.csv\")\n",
    "    if not os.path.exists(path):\n",
    "        return pd.DataFrame()\n",
    "    df = pd.read_csv(path, usecols=[\"open_time\",\"close\"])\n",
    "    df = ensure_utc_index(df, \"open_time\")\n",
    "    # клип на всякий случай\n",
    "    t0 = pd.Timestamp(START_DATE + \" 00:00:00\", tz=\"UTC\")\n",
    "    t1 = pd.Timestamp(END_DATE   + \" 23:59:59\", tz=\"UTC\")\n",
    "    df = df.loc[(df.index>=t0) & (df.index<=t1)]\n",
    "    return df.rename(columns={\"close\": symbol})[[symbol]]\n",
    "\n",
    "# --- загрузка BTC (из твоих дневных файлов) ---\n",
    "btc = load_btc_daily_concat()\n",
    "\n",
    "# --- загрузка остальных монет (из единого 15s файла по каждой) ---\n",
    "symbols_other = [\n",
    "    \"ETHUSDT\",\"BNBUSDT\",\"SOLUSDT\",\"XRPUSDT\",\"DOGEUSDT\",\"ADAUSDT\",\"AVAXUSDT\",\"TRXUSDT\",\"TONUSDT\",\"LINKUSDT\",\n",
    "    \"MATICUSDT\",\"DOTUSDT\",\"HYPEUSDT\",\"BCHUSDT\",\"XLMUSDT\",\"LEOUSDT\",\"SUIUSDT\",\"XMRUSDT\",\"SHIBUSDT\",\"UNIUSDT\"\n",
    "]\n",
    "\n",
    "merged = btc.copy()\n",
    "present = []\n",
    "for s in symbols_other:\n",
    "    dfc = load_other_symbol(s)\n",
    "    if dfc.empty:\n",
    "        continue\n",
    "    merged = merged.join(dfc, how=\"inner\")\n",
    "    present.append(s)\n",
    "\n",
    "print(f\"Merged shape: {merged.shape}. Взяты альты: {present}\")\n",
    "\n",
    "# --- корреляции по close (15s) ---\n",
    "corr = merged.corr(method=\"pearson\")\n",
    "\n",
    "# топ-10 с BTC (исключая сам BTC)\n",
    "top10 = corr[BENCH].drop(index=BENCH).sort_values(ascending=False).head(10)\n",
    "print(\"\\nTop-10 коррелирующих с BTCUSDT (close, 15s):\")\n",
    "print(top10)\n",
    "\n",
    "# --- heatmap по BTC + топ-10 ---\n",
    "focus = [BENCH] + top10.index.tolist()\n",
    "mat = corr.loc[focus, focus]\n",
    "\n",
    "plt.figure(figsize=(10, 8))\n",
    "im = plt.imshow(mat.values, vmin=-1, vmax=1, aspect='auto')\n",
    "plt.colorbar(im, fraction=0.046, pad=0.04)\n",
    "plt.xticks(range(mat.shape[1]), mat.columns, rotation=45, ha='right')\n",
    "plt.yticks(range(mat.shape[0]), mat.index)\n",
    "plt.title(f\"Корреляции по close (15s), {START_DATE}..{END_DATE}\")\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
  },
