     ]
    }
   ],
   "source": [
    "class Args:\n",
    "    input_dir = \"BTCUSDT-04-2025\"\n",
    "    output_dir = \"MyDatasets/15s\"\n",
    "    pattern = \"BTCUSDT-1s-2025-04-*.csv\"\n",
    "    tz = \"UTC\"\n",
    "\n",
    "args = Args()\n",
    "\n",
    "def _parse_timestamp_series(series: pd.Series) -> pd.DatetimeIndex:\n",
    "    \"\"\"Detect unit (us/ms/s) or ISO and return pandas datetime index (naive).\"\"\"\n",
    "    if pd.api.types.is_numeric_dtype(series):\n",
    "        v = int(series.iloc[0])\n",
    "        if v >= 10**15:\n",
    "            idx = pd.to_datetime(series.astype(\"int64\"), unit=\"us\")\n",
    "        elif v >= 10**12:\n",
    "            idx = pd.to_datetime(series.astype(\"int64\"), unit=\"ms\")\n",
    "        elif v >= 10**9:\n",
    "            idx = pd.to_datetime(series.astype(\"int64\"), unit=\"s\")\n",
    "        else:\n",
    "            idx = pd.to_datetime(series.astype(\"int64\"), unit=\"s\")\n",
    "        return pd.DatetimeIndex(idx)\n",
    "    return pd.to_datetime(series.astype(str), errors=\"raise\", infer_datetime_format=True)\n",
    "\n",
    "\n",
    "def _detect_time_index(df: pd.DataFrame) -> pd.DatetimeIndex:\n",
    "    \"\"\"Try common time column names; else use the first column.\"\"\"\n",
    "    cand_names = [\n",
    "        \"open_time\", \"timestamp\", \"time\", \"date\", \"start_time\", \"t\",\n",
    "        \"open time\", \"openTime\", \"Open time\"\n",
    "    ]\n",
    "    lower_map = {c.lower(): c for c in df.columns}\n",
    "    for c in cand_names:\n",
    "        if c in lower_map:\n",
    "            return _parse_timestamp_series(df[lower_map[c]])\n",
    "    return _parse_timestamp_series(df.iloc[:, 0])\n",
    "\n",
    "\n",
    "def _normalize_ohlcv(df: pd.DataFrame) -> pd.DataFrame:\n",
    "    \"\"\"\n",
    "    Return dataframe with columns: open, high, low, close, volume, [quote_volume?].\n",
    "    If file looks like 12-column Binance export without headers — assign standard names.\n",
    "    \"\"\"\n",
    "    if len(df.columns) == 12 and str(df.columns[0]).isdigit():\n",
    "        df = df.copy()\n",
    "        df.columns = [\n",
    "            \"open_time\", \"open\", \"high\", \"low\", \"close\", \"volume\",\n",
    "            \"close_time\", \"quote_volume\", \"trades\",\n",
    "            \"taker_base\", \"taker_quote\", \"ignore\"\n",
    "        ]\n",
    "\n",
    "    lower = {c.lower(): c for c in df.columns}\n",
    "    mapping: Dict[str, List[str]] = {\n",
    "        \"open\":   [\"open\", \"o\"],\n",
    "        \"high\":   [\"high\", \"h\"],\n",
    "        \"low\":    [\"low\", \"l\"],\n",
    "        \"close\":  [\"close\", \"c\", \"close_price\", \"price\"],\n",
    "        \"volume\": [\"volume\", \"v\", \"base_volume\", \"vol\"],\n",
    "    }\n",
    "    colmap: Dict[str, str] = {}\n",
    "    for std, cands in mapping.items():\n",
    "        for cand in cands:\n",
    "            if cand.lower() in lower:\n",
    "                colmap[std] = lower[cand.lower()]\n",
    "                break\n",
    "    missing = [k for k in [\"open\", \"high\", \"low\", \"close\", \"volume\"] if k not in colmap]\n",
    "    if missing:\n",
    "        raise ValueError(f\"Missing OHLCV columns: {missing}. Available: {list(df.columns)}\")\n",
    "\n",
    "    out = df[[colmap[\"open\"], colmap[\"high\"], colmap[\"low\"], colmap[\"close\"], colmap[\"volume\"]]].copy()\n",
    "    out.columns = [\"open\", \"high\", \"low\", \"close\", \"volume\"]\n",
    "\n",
    "    # Optional quote volume\n",
    "    if \"quote_volume\" in lower:\n",
    "        out[\"quote_volume\"] = df[lower[\"quote_volume\"]]\n",
    "    elif \"taker_quote\" in df.columns:\n",
    "        out[\"quote_volume\"] = df[\"taker_quote\"]\n",
    "    return out\n",
    "\n",
    "\n",
    "def _resample_15s(ohlcv_1s: pd.DataFrame) -> pd.DataFrame:\n",
    "    \"\"\"Resample to 15s: open=first, high=max, low=min, close=last, volume=sum (quote_volume=sum if present).\"\"\"\n",
    "    ohlcv_1s = ohlcv_1s.loc[~ohlcv_1s.index.duplicated(keep=\"last\")].sort_index()\n",
    "    agg = {\"open\": \"first\", \"high\": \"max\", \"low\": \"min\", \"close\": \"last\", \"volume\": \"sum\"}\n",
    "    if \"quote_volume\" in ohlcv_1s.columns:\n",
    "        agg[\"quote_volume\"] = \"sum\"\n",
    "    out = ohlcv_1s.resample(\"15S\", label=\"right\", closed=\"right\").agg(agg)\n",
    "    return out.dropna(subset=[\"open\", \"high\", \"low\", \"close\"])\n",
    "\n",
    "\n",
    "def _clip_to_day(day_str: str, tz: Optional[str]) -> Tuple[pd.Timestamp, pd.Timestamp]:\n",
    "    \"\"\"Build [day_start, day_end) from YYYY-MM-DD and optional tz.\"\"\"\n",
    "    day_start = pd.Timestamp(f\"{day_str} 00:00:00\")\n",
    "    if tz:\n",
    "        day_start = day_start.tz_localize(tz)\n",
    "    day_end = day_start + pd.Timedelta(days=1)\n",
    "    return day_start, day_end\n",
    "\n",
    "def extract_date_from_filename(filename: str) -> str:\n",
    "    \"\"\"\n",
    "    Достаёт 'YYYY-MM-DD' из имени файла. Бросает ошибку, если даты нет.\n",
    "    Пример: 'BTCUSDT-1s-2025-04-24.csv' -> '2025-04-24'\n",
    "    \"\"\"\n",
    "    m = re.search(r\"\\d{4}-\\d{2}-\\d{2}\", filename)\n",
    "    if not m:\n",
    "        raise ValueError(f\"Cannot find YYYY-MM-DD in filename: {filename}\")\n",
    "    return m.group(0)\n",
    "\n",
    "\n",
    "\n",
    "def process_file(path: str, out_dir: str, tz: Optional[str]) -> Tuple[Optional[dict], Optional[str]]:\n",
    "    # read .csv\n",
    "    try:\n",
    "        df = pd.read_csv(path, header=0)\n",
    "    except Exception:\n",
    "        df = pd.read_csv(path, header=0, engine=\"python\")\n",
    "\n",
    "    idx = _detect_time_index(df)\n",
    "    if tz:\n",
    "        if idx.tz is None:\n",
    "            idx = idx.tz_localize(tz)\n",
    "        else:\n",
    "            idx = idx.tz_convert(tz)\n",
    "\n",
    "    ohlcv = _normalize_ohlcv(df)\n",
    "    ohlcv.index = idx\n",
    "\n",
    "    base = os.path.basename(path)\n",
    "    date_part = extract_date_from_filename(base)\n",
    "    day_start, day_end = _clip_to_day(date_part, tz)\n",
    "    mask = (ohlcv.index >= day_start) & (ohlcv.index < day_end)\n",
    "    ohlcv = ohlcv.loc[mask]\n",
    "\n",
    "\n",
    "    out15 = _resample_15s(ohlcv)\n",
    "\n",
    "    os.makedirs(out_dir, exist_ok=True)\n",
    "    out_name = base.replace(\"-1s-\", \"-15s-\").replace(\".csv.gz\", \".csv\")\n",
    "    out_path = os.path.join(out_dir, out_name)\n",
    "    out15.to_csv(out_path, index_label=\"open_time\")\n",
    "\n",
    "    return ({\n",
    "        \"input_file\": base,\n",
    "        \"output_file\": os.path.basename(out_path),\n",
    "        \"rows_1s\": int(len(ohlcv)),\n",
    "        \"rows_15s\": int(len(out15)),\n",
    "        \"start_1s\": str(ohlcv.index.min()) if len(ohlcv) else \"\",\n",
    "        \"end_1s\": str(ohlcv.index.max()) if len(ohlcv) else \"\",\n",
    "        \"start_15s\": str(out15.index.min()) if len(out15) else \"\",\n",
    "        \"end_15s\": str(out15.index.max()) if len(out15) else \"\",\n",
    "    }, None)\n",
    "\n",
    "\n",
    "def main():\n",
    "    #args = parse_args()\n",
    "    in_glob = os.path.join(args.input_dir, args.pattern)\n",
    "    files = sorted(set(glob.glob(in_glob)) | set(glob.glob(in_glob + \".gz\")))\n",
    "\n",
    "    if not files:\n",
    "        print(f\"[!] No files matched pattern: {in_glob}\")\n",
    "        sys.exit(1)\n",
    "\n",
    "    results, errors = [], []\n",
    "    for p in files:\n",
    "        print(f\"[+] Processing: {p}\")\n",
    "        try:\n",
    "            summary, err = process_file(p, args.output_dir, args.tz)\n",
    "            if err:\n",
    "                print(f\"[x] {p} -> {err}\")\n",
    "                errors.append(f\"{os.path.basename(p)} -> {err}\")\n",
    "            else:\n",
    "                results.append(summary)\n",
    "        except Exception as e:\n",
    "            print(f\"[x] {p} -> {e}\")\n",
    "            errors.append(f\"{os.path.basename(p)} -> {e}\")\n",
    "\n",
    "    # manifest\n",
    "    if results:\n",
    "        manifest = pd.DataFrame(results)\n",
    "        manifest_path = os.path.join(args.output_dir, \"manifest_15s.csv\")\n",
    "        manifest.to_csv(manifest_path, index=False)\n",
    "        print(f\"[✓] Wrote manifest: {manifest_path}\")\n",
    "    else:\n",
    "        print(\"[!] No successful files to summarize.\")\n",
    "\n",
    "    if errors:\n",
    "        print(\"\\n[!] Errors:\")\n",
    "        for e in errors:\n",
    "            print(\"   \", e)\n",
    "    else:\n",
    "        print(\"[✓] Completed with no errors.\")\n",
    "\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "997b1299-a308-42df-a2b3-cc0cfda0a487",
   "metadata": {},
   "outputs": [],
   "source": [
    "Ищем дневную волатильность и визуализируем ее"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "2a347826-e053-4e86-baac-61fcd0ac5b5c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[✓] Saved daily metrics to daily_vol_BTCUSDT_2025-04.csv\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[✓] Saved plot to daily_vol_BTCUSDT_2025-04.png\n",
      "\n",
      "[!] Найдены аномальные дни:\n",
      "                  rv  n_bars  max_abs_ret_15s  missing_ratio                      anomaly_reason\n",
      "date                                                                                            \n",
      "2025-04-07  0.060823    5759         0.009371       0.000174  высокая RV (z=3.16); выброс по IQR\n",
      "2025-04-09  0.049515    5759         0.005412       0.000174                 высокая RV (z=2.18)\n",
      "2025-04-22  0.030739    5759         0.013382       0.000174   экстремальный 15s-скачок (0.0134)\n"
     ]
    }
   ],
   "source": [
    "# === Конфигурация путей ===\n",
    "INPUT_DIR = \"15s\"                     # папка с BTCUSDT-15s-2025-04-*.csv\n",
    "PATTERN   = \"BTCUSDT-15s-2025-04-*.csv\"          # маска имен\n",
    "SYMBOL    = \"BTCUSDT\"\n",
    "MONTH_TAG = \"2025-04\"                            # метка месяца (для заголовков/файлов вывода)\n",
    "OUT_CSV   = f\"daily_vol_{SYMBOL}_{MONTH_TAG}.csv\"\n",
    "OUT_PNG   = f\"daily_vol_{SYMBOL}_{MONTH_TAG}.png\"\n",
    "\n",
    "\n",
    "# === Утилиты ===\n",
    "def read_15s_file(path: str) -> pd.DataFrame:\n",
    "    \"\"\"Читает 15s CSV: ожидает колонки open_time, open, high, low, close, volume[, quote_volume].\"\"\"\n",
    "    df = pd.read_csv(path)\n",
    "    # Приводим время к pandas datetime\n",
    "    df['open_time'] = pd.to_datetime(df['open_time'], errors='coerce')\n",
    "    df = df.dropna(subset=['open_time'])\n",
    "    df = df.sort_values('open_time').set_index('open_time')\n",
    "    # Приводим цены/объёмы к float\n",
    "    for c in ['open','high','low','close','volume']:\n",
    "        if c in df.columns:\n",
    "            df[c] = pd.to_numeric(df[c], errors='coerce')\n",
    "    return df\n",
    "\n",
    "def extract_date_from_filename(filename: str) -> str:\n",
    "    \"\"\"Ищет YYYY-MM-DD в имени файла.\"\"\"\n",
    "    m = re.search(r\"\\d{4}-\\d{2}-\\d{2}\", filename)\n",
    "    if not m:\n",
    "        raise ValueError(f\"Cannot find YYYY-MM-DD in filename: {filename}\")\n",
    "    return m.group(0)\n",
    "\n",
    "def realized_vol_from_15s(close_series: pd.Series) -> float:\n",
    "    \"\"\"\n",
    "    Реализованная дневная волатильность (realized volatility):\n",
    "    RV_day = sqrt( sum_{t in day} (log C_t - log C_{t-1})^2 ).\n",
    "    Возвращает число в 'абсолютных' единицах (например, 0.02 = 2%).\n",
    "    \"\"\"\n",
    "    r = np.log(close_series).diff().dropna()\n",
    "    rv = np.sqrt(np.sum(r*r))\n",
    "    return float(rv)\n",
    "\n",
    "def parkinson_vol_from_hilo(high: pd.Series, low: pd.Series) -> float:\n",
    "    \"\"\"\n",
    "    Паркинсон для справки (не обязателен): использует high/low.\n",
    "    sigma_P = sqrt(1/(4 ln 2) * mean( ln(High/Low)^2 ) ).\n",
    "    На 15s-скейле это скорее индикативно.\n",
    "    \"\"\"\n",
    "    x = np.log(high/low).dropna()\n",
    "    const = 1.0/(4.0*np.log(2.0))\n",
    "    return float(np.sqrt(const * np.mean(x*x)))\n",
    "\n",
    "def summarize_day(df_day: pd.DataFrame) -> dict:\n",
    "    \"\"\"Возвращает словарь метрик по дню.\"\"\"\n",
    "    metrics = {}\n",
    "    metrics['n_bars'] = len(df_day)\n",
    "    metrics['first_ts'] = df_day.index.min()\n",
    "    metrics['last_ts']  = df_day.index.max()\n",
    "    metrics['close_first'] = float(df_day['close'].iloc[0])\n",
    "    metrics['close_last']  = float(df_day['close'].iloc[-1])\n",
    "\n",
    "    # Реализованная волатильность (по close)\n",
    "    metrics['rv'] = realized_vol_from_15s(df_day['close'])\n",
    "\n",
    "    # Доп. метрики для объяснения аномалий\n",
    "    r = np.log(df_day['close']).diff().dropna()\n",
    "    metrics['max_abs_ret_15s'] = float(np.abs(r).max()) if len(r) else np.nan\n",
    "    metrics['median_abs_ret_15s'] = float(np.median(np.abs(r))) if len(r) else np.nan\n",
    "    metrics['sum_volume'] = float(df_day['volume'].sum()) if 'volume' in df_day.columns else np.nan\n",
    "\n",
    "    # Альтернатива (необязательно): Паркинсон\n",
    "    try:\n",
    "        metrics['parkinson'] = parkinson_vol_from_hilo(df_day['high'], df_day['low'])\n",
    "    except Exception:\n",
    "        metrics['parkinson'] = np.nan\n",
    "\n",
    "    # Доля пропусков относительно теоретических 5760 баров\n",
    "    metrics['missing_ratio'] = max(0.0, 1.0 - metrics['n_bars'] / 5760.0)\n",
    "\n",
    "    return metrics\n",
    "\n",
    "def flag_anomalies(df_daily: pd.DataFrame) -> pd.DataFrame:\n",
    "    \"\"\"\n",
    "    Помечает аномальные дни по нескольким критериям:\n",
    "    - Z-score realized vol > 2.0\n",
    "    - IQR: выше Q3 + 1.5*IQR\n",
    "    - Мало баров (< 95% от 5760)\n",
    "    - Существуют экстремальные 15s-сдвиги (max_abs_ret_15s в топ-квантили)\n",
    "    \"\"\"\n",
    "    out = df_daily.copy()\n",
    "\n",
    "    # Z-score по rv\n",
    "    mu, sd = out['rv'].mean(), out['rv'].std(ddof=1)\n",
    "    out['zscore_rv'] = (out['rv'] - mu) / sd if sd > 0 else 0.0\n",
    "\n",
    "    # Правило ящика\n",
    "    q1, q3 = out['rv'].quantile(0.25), out['rv'].quantile(0.75)\n",
    "    iqr = q3 - q1\n",
    "    high_iqr = q3 + 1.5 * iqr\n",
    "\n",
    "    # Порог по числу баров\n",
    "    out['low_bars'] = out['n_bars'] < int(0.95 * 5760)\n",
    "\n",
    "    # Порог по экстремальному 15s скачку: верхние 2% квантиль по месяцу\n",
    "    thr_jump = out['max_abs_ret_15s'].quantile(0.98)\n",
    "    out['big_jump'] = out['max_abs_ret_15s'] >= thr_jump\n",
    "\n",
    "    # Главный флаг \"аномалия\"\n",
    "    out['anomaly'] = (\n",
    "        (out['zscore_rv'] >= 2.0) |      # очень высокая дневная вола\n",
    "        (out['rv'] >= high_iqr)    |      # выброс по IQR\n",
    "        (out['low_bars'])          |      # не хватает баров\n",
    "        (out['big_jump'])                 # очень большой 15s скачок\n",
    "    )\n",
    "\n",
    "    # Текстовое объяснение по каждому дню\n",
    "    reasons = []\n",
    "    for _, row in out.iterrows():\n",
    "        rs = []\n",
    "        if row['zscore_rv'] >= 2.0:\n",
    "            rs.append(f\"высокая RV (z={row['zscore_rv']:.2f})\")\n",
    "        if row['rv'] >= high_iqr:\n",
    "            rs.append(\"выброс по IQR\")\n",
    "        if row['low_bars']:\n",
    "            rs.append(f\"малое число баров ({row['n_bars']}/5760)\")\n",
    "        if row['big_jump']:\n",
    "            rs.append(f\"экстремальный 15s-скачок ({row['max_abs_ret_15s']:.4f})\")\n",
    "        reasons.append(\"; \".join(rs) if rs else \"\")\n",
    "    out['anomaly_reason'] = reasons\n",
    "\n",
    "    return out\n",
    "\n",
    "# === Чтение всех файлов и расчёт по дням ===\n",
    "files = sorted(glob.glob(os.path.join(INPUT_DIR, PATTERN)))\n",
    "if not files:\n",
    "    raise SystemExit(f\"[!] No files matched: {os.path.join(INPUT_DIR, PATTERN)}\")\n",
    "\n",
    "rows = []\n",
    "for path in files:\n",
    "    day_str = extract_date_from_filename(os.path.basename(path))  # YYYY-MM-DD\n",
    "    df = read_15s_file(path)\n",
    "    if df.empty:\n",
    "        continue\n",
    "    \n",
    "    day_start = pd.Timestamp(day_str + \" 00:00:00\", tz=\"UTC\")\n",
    "    day_end   = day_start + pd.Timedelta(days=1)\n",
    "\n",
    "    df = df.loc[(df.index >= day_start) & (df.index < day_end)]\n",
    "    if df.empty:\n",
    "        continue\n",
    "    metrics = summarize_day(df)\n",
    "    metrics['date'] = day_str\n",
    "    rows.append(metrics)\n",
    "\n",
    "daily = pd.DataFrame(rows).sort_values('date').set_index('date')\n",
    "\n",
    "# === Поиск аномалий ===\n",
    "daily = flag_anomalies(daily)\n",
    "\n",
    "# === Сохранение результатов ===\n",
    "daily.to_csv(OUT_CSV, index=True)\n",
    "print(f\"[✓] Saved daily metrics to {OUT_CSV}\")\n",
    "\n",
    "# === Визуализация ===\n",
    "plt.figure(figsize=(12, 5))\n",
    "plt.plot(pd.to_datetime(daily.index), daily['rv'], marker='o', linewidth=1)\n",
    "plt.title(f\"{SYMBOL} — Дневная реализованная волатильность ({MONTH_TAG})\")\n",
    "plt.xlabel(\"Дата\")\n",
    "plt.ylabel(\"Realized Vol (sqrt(sum r^2), за день)\")\n",
    "plt.grid(True, alpha=0.3)\n",
    "\n",
    "# Подсветим аномалии на графике\n",
    "anom = daily[daily['anomaly']]\n",
    "if not anom.empty:\n",
    "    plt.scatter(pd.to_datetime(anom.index), anom['rv'], s=80, marker='^')\n",
    "    # Подписи над точками\n",
    "    for d, y, txt in zip(pd.to_datetime(anom.index), anom['rv'], anom['anomaly_reason']):\n",
    "        plt.annotate(txt, (d, y), xytext=(5, 8), textcoords='offset points', fontsize=8)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.savefig(OUT_PNG, dpi=150)\n",
    "plt.show()\n",
    "print(f\"[✓] Saved plot to {OUT_PNG}\")\n",
    "\n",
    "# === Краткий отчёт об аномалиях ===\n",
    "if not anom.empty:\n",
    "    print(\"\\n[!] Найдены аномальные дни:\")\n",
    "    disp_cols = ['rv','n_bars','max_abs_ret_15s','missing_ratio','anomaly_reason']\n",
    "    print(anom[disp_cols].round(6).to_string())\n",
    "else:\n",
    "    print(\"\\n[✓] Аномалий не найдено по заданным критериям.\")\n"
   ]
  },
